{
  "name": "covid19-messenger-bot",
  "description": "Messenger chatbot that helps in tracking COVID19 cases, recoveries & deaths",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/Oplan-Hope/covid19-messenger-bot",
  "author": "Hope",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"{api,controllers,routes,resources,utils}/**/*.{css,js,json}\"",
    "start": "cross-env NODE_PATH=./ node server.js",
    "dev-watch": "cross-env NODE_PATH=./ nodemon server.js",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --colors",
    "build:watch": "yarn build:dev -- --watch",
    "build:prod": "cross-env NODE_ENV=production webpack --no-progress --hide-modules --colors"
  },
  "dependencies": {
    "@babel/preset-react": "^7.9.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "geolib": "^3.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "prettier": "^2.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
